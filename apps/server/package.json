{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prisma": "prisma",
    "predev": "pnpm --filter @acme/shared run build",
    "dev": "tsx watch src/index.ts",
    "prebuild": "pnpm --filter @acme/shared run build",
    "build": "tsup --config tsup.config.ts",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest",
    "premigrate": "pnpm --filter @acme/shared run build",
    "migrate": "prisma migrate deploy",
    "preseed": "pnpm --filter @acme/shared run build",
    "seed": "prisma db seed",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@acme/shared": "workspace:*",
    "@fastify/cookie": "9.3.1",
    "@fastify/cors": "9.0.1",
    "@fastify/multipart": "7.7.3",
    "@fastify/sensible": "5.6.0",
    "@fastify/static": "7.0.2",
    "@fastify/websocket": "8.3.0",
    "@prisma/client": "5.12.1",
    "@socket.io/redis-adapter": "8.2.1",
    "bcryptjs": "2.4.3",
    "cookie": "0.6.0",
    "dotenv": "16.4.5",
    "fastify": "4.26.2",
    "@fastify/rate-limit": "6.0.0",
    "fastify-type-provider-zod": "4.0.1",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.2",
    "mime-types": "2.1.35",
    "pino": "8.17.0",
    "sharp": "0.33.2",
    "socket.io": "4.7.5",
    "uuid": "9.0.1",
    "zod": "3.23.8",
    "prisma": "5.12.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.5.4",
    "@types/jsonwebtoken": "9.0.5",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.11.19",
    "@types/uuid": "9.0.7",
    "eslint": "8.57.0",
    "pino-pretty": "10.3.1",
    "tsup": "7.2.0",
    "tsx": "4.7.0",
    "typescript": "5.4.3",
    "vitest": "1.4.0"
  }
}
